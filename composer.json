{
    "name": "orgmanager/orgmanager",
    "type": "project",
    "description": "Supercharge you GitHub organizations!",
    "keywords": [
        "orgmanager",
        "github",
        "organizations",
        "supercharge",
        "invite",
        "invites",
        "php",
        "laravel"
    ],
    "license": "MPL-2.0",
    "require": {
        "php": "^7.1.3",
        "fideloper/proxy": "4.2.0",
        "google/recaptcha": "1.2.2",
        "graham-campbell/github": "7.8.0",
        "guzzlehttp/guzzle": "6.3.3",
        "knplabs/github-api": "2.11.0",
        "laravel/framework": "5.8.32",
        "laravel/socialite": "3.3.0",
        "laravel/tinker": "1.0.10",
        "league/commonmark": "0.18.5",
        "orgmanager/orgmanager-logos": "1.1",
        "php-http/cache-plugin": "1.6.0",
        "php-http/guzzle6-adapter": "1.1.1",
        "sentry/sentry-laravel": "0.8.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "1.3.0",
        "filp/whoops": "2.5.0",
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.3",
        "phpunit/phpunit": "8.3.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan migrate --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
